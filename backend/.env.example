# Database configuration
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/nextflow_platform

# Authentication
SECRET_KEY=your_secret_key_here_change_in_production
ACCESS_TOKEN_EXPIRE_MINUTES=30

